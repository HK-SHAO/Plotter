run/low_processor_mode=true
boot_splash/image="res://shared/image/boot_splash.png"
boot_splash/bg_color=Color(0.301961, 0.301961, 0.301961, 1)
config/icon="res://shared/image/icon.svg"

[audio]

driver/enable_input=true

